{
  "name": "@collectivehealth/unity",
  "description": "A library to make unit testing Angular 1 applications suck less.",
  "version": "1.2.0",
  "contributors": [
    {
      "name": "Joshua Martin",
      "email": "joshua@collectivehealth.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collectivehealth/unity"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "docs:build": "rimraf docs && jsdoc --configure .jsdoc.json --verbose",
    "docs:serve": "npm run docs:build && open http://localhost:9000 & http-server ./docs --silent -p 9000",
    "build": "run-p clean lint && babel src --out-dir dist",
    "build:watch": "watch \"npm run build\" ./src/",
    "lint": "eslint .",
    "prepush": "npm run lint",
    "prepublish": "run-s lint build"
  },
  "peerDependencies": {
    "angular": ">=1.3",
    "angular-mocks": ">=1.3"
  },
  "devDependencies": {
    "@collectivehealth/code-quality": "1.x",
    "angular": "1.3.x",
    "angular-mocks": "1.3.x",
    "babel-cli": "6.x",
    "babel-preset-es2015": "6.x",
    "docdash": "0.3.x",
    "eslint-plugin-import": "2.2.x",
    "http-server": "0.9.x",
    "husky": "0.13.x",
    "jsdoc": "3.4.x",
    "npm-run-all": "4.0.x",
    "rimraf": "2.5.x",
    "watch": "1.0.x"
  }
}
